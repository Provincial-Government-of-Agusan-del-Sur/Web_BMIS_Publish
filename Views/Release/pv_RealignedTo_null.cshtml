    function fn_History_realign_from() {

        var url = "@Url.Action("Realigned_From", "Release")";


        var office_id = $('#office_realigned_to_null').val()
        var program_id = $('#Program_realigned_to_null').val()
        //var ooe_id = $('#ooe_id_realigned_to_null').val()
        var account_id = $('#account_list_realigned_to_null').val()
       
        if (office_id == '' || office_id == 0 || office_id == null) {
            swal({
                title: "Error!",
                text: "PLEASE Select Office",
                timer: 3000,
                type: "error",
                showConfirmButton: false
            })
        } else {



            $.get(url, { office_id: office_id, program_id: program_id, account_id: account_id }, function (d) {
                $('#history_from').html(d);
            })

            $('#history_from').data('kendoWindow').title("<i class='fa fa-navicon'> </i> Realigned From").center().open();
        }
    }
    function fn_History_realign_to() {
        var url = "@Url.Action("Realigned_To", "Release")";


