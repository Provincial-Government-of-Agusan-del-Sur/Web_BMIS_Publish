               // fn_refreshtext_null();
                reffff();
                document.getElementById("amounttorealign_null").value = "";
                document.getElementById("toAmount_null").value = "";

                if (e == "1") {
                    swal({
                        title: "Success!",
                        text: "New Realignment has been Saved.",
                        timer: 2000,
                        type: "success",
                        showConfirmButton: false
                    })
                    $('#GeneralFund_Grid').data('kendoGrid').dataSource.read();
                } else {
                    swal({
                        title: "Error!",
                        text: "System Error! Please try again.",
                        timer: 2000,
                        type: "error",
                        showConfirmButton: false
                    })
                }


             });
            });

            }
        }


    }
