    <script>
        function hideSeriesAll() {
            for (var i = 0; i <= 82; i++) {
                if ($("#chartLine").getKendoChart()._sourceSeries[i].visible) {
                    $("#chartLine").getKendoChart()._legendItemClick(i);
                }
            }
        }
        function showSeriesAll() {
            for (var i = 0; i <= 82; i++) {
                if (!$("#chartLine").getKendoChart()._sourceSeries[i].visible) {
                    $("#chartLine").getKendoChart()._legendItemClick(i);
                }
            }
        }
    </script>
